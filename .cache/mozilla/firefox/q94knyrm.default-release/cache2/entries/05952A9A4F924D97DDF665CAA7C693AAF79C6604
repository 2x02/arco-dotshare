<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Mod1</span><span class="o">+</span><span class="n">114 split h</span>

<span class="c1"># split in vertical orientation (mod+v)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">55 split v</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Mod1</span><span class="o">+</span><span class="n">116 split v</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Mod1</span><span class="o">+</span><span class="n">111 split v</span>

<span class="c1"># enter fullscreen mode for the focused container (mod+f)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">41 fullscreen</span>

<span class="c1"># change container layout (stacked (mod+s), tabbed (mod+t), default(mod+e))</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">39 layout stacking</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">28 layout tabbed</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">40 layout default</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">25 layout toggle split</span>

<span class="c1"># toggle tiling / floating (mod+shitf+space)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">65 floating toggle</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Mod1</span><span class="o">+</span><span class="n">65 floating toggle</span>

<span class="c1"># change focus between tiling / floating windows (mod+space)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">65 focus mode_toggle</span>

<span class="c1"># focus the parent container (mod+a)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">38 focus parent</span>

<span class="c1"># focus the child container (mod+z)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">52 focus child</span>

<span class="c1"># switch to workspace</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">10 workspace 1</span><span class="o">:</span><span class="n">term</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">11 workspace 2</span><span class="o">:</span><span class="n">www</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">12 workspace 3</span><span class="o">:</span><span class="n">vbox</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">13 workspace 4</span><span class="o">:</span><span class="n">vnc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">14 workspace 5</span><span class="o">:</span><span class="n">doc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">15 workspace 6</span><span class="o">:</span><span class="n">img</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">16 workspace 7</span><span class="o">:</span><span class="n">misc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">17 workspace </span><span class="mi">8</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">18 workspace </span><span class="mi">9</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">19 workspace </span><span class="mi">10</span>

<span class="c1"># move focused container to workspace</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">10 move container to workspace 1</span><span class="o">:</span><span class="n">term</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">11 move container to workspace 2</span><span class="o">:</span><span class="n">www</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">12 move container to workspace 3</span><span class="o">:</span><span class="n">vbox</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">13 move container to workspace 4</span><span class="o">:</span><span class="n">vnc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">14 move container to workspace 5</span><span class="o">:</span><span class="n">doc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">15 move container to workspace 6</span><span class="o">:</span><span class="n">img</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">16 move container to workspace 7</span><span class="o">:</span><span class="n">misc</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">17 move container to workspace </span><span class="mi">8</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">18 move container to workspace </span><span class="mi">9</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">19 move container to workspace </span><span class="mi">10</span>

<span class="c1"># reload the configuration file (mod+shitf+c)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">54 reload</span>
<span class="c1"># restart i3 inplace (preserves your layout/session, can be used to upgrade i3) (mod+shitf+r)</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">Shift</span><span class="o">+</span><span class="n">27 restart</span>
<span class="c1"># exit i3 (logs you out of your X session) (mod+shitf+e)</span>
<span class="c1">#bindcode $mod+Shift+26 exec &quot;i3-nagbar -t warning -m &#39;You pressed the exit shortcut. Do you really want to exit i3? This will end your X session.&#39; -b &#39;Yes, exit i3&#39; &#39;i3-msg exit&#39;&quot;</span>

<span class="c1"># resize window (you can also use the mouse for that) (mod+r)</span>
<span class="k">mode</span><span class="n"> </span><span class="s">&quot;resize&quot;</span><span class="n"> </span><span class="o">{</span>
<span class="n">  </span><span class="c1"># These bindings trigger as soon as you enter the resize mode</span>

<span class="n">  </span><span class="c1"># Pressing left will shrink the windowâ€™s width.</span>
<span class="n">  </span><span class="c1"># Pressing right will grow the windowâ€™s width.</span>
<span class="n">  </span><span class="c1"># Pressing up will shrink the windowâ€™s height.</span>
<span class="n">  </span><span class="c1"># Pressing down will grow the windowâ€™s height.</span>
<span class="n">  bindcode </span><span class="mi">44</span><span class="n"> resize shrink width </span><span class="mi">10</span><span class="n"> px or </span><span class="mi">10</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">45</span><span class="n"> resize grow height </span><span class="mi">10</span><span class="n"> px or </span><span class="mi">10</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">46</span><span class="n"> resize shrink height </span><span class="mi">10</span><span class="n"> px or </span><span class="mi">10</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">47</span><span class="n"> resize grow width </span><span class="mi">10</span><span class="n"> px or </span><span class="mi">10</span><span class="n"> ppt</span>

<span class="n">  </span><span class="c1"># same bindings, but for the arrow keys</span>
<span class="n">  bindcode </span><span class="mi">113</span><span class="n"> resize shrink width </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">116</span><span class="n"> resize grow height </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">111</span><span class="n"> resize shrink height </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>
<span class="n">  bindcode </span><span class="mi">114</span><span class="n"> resize grow width </span><span class="mi">1</span><span class="n"> px or </span><span class="mi">1</span><span class="n"> ppt</span>

<span class="n">  </span><span class="c1"># back to normal: Enter or Escape</span>
<span class="n">  bindcode </span><span class="mi">36</span><span class="n"> mode </span><span class="s">&quot;default&quot;</span>
<span class="n">  bindcode </span><span class="mi">9</span><span class="n"> mode </span><span class="s">&quot;default&quot;</span>
<span class="o">}</span>
<span class="k">bindcode</span><span class="n"> </span><span class="o">$</span><span class="n">mod</span><span class="o">+</span><span class="n">27 mode </span><span class="s">&quot;resize&quot;</span>

<span class="c1"># My bar</span>
<span class="k">bar</span><span class="n"> </span><span class="o">{</span>
<span class="n">  i3bar_command ~/.i3/lemonbar/i3_lemonbar.sh</span>
<span class="o">}</span>
