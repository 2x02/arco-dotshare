<span class="c1"># TMUX Launch</span>
<span class="k">alias</span><span class="n"> </span><span class="k">tmux</span><span class="o">=</span><span class="s">&#39;tmux -2&#39;</span>
<span class="k">function</span><span class="n"> m </span><span class="o">{</span>
<span class="n">    if [[ </span><span class="nf">-z</span><span class="n"> </span><span class="o">$</span><span class="n">TMUX ]]</span><span class="o">;</span><span class="n"> then</span>
<span class="n">        </span><span class="c1"># Attempt to discover a detached session and attach it, else create a new session</span>
<span class="n">        </span><span class="c1"># CURRENT_USER=$(whoami)</span>
<span class="n">        </span><span class="k">TMUX_CON_SESSION</span><span class="o">=</span><span class="n">__dev__</span>
<span class="n">        if tmux has-session </span><span class="nf">-t</span><span class="n"> </span><span class="o">$</span><span class="n">TMUX_CON_SESSION 2&gt;/dev/null</span><span class="o">;</span><span class="n"> then</span>
<span class="n">            tmux </span><span class="mi">-2</span><span class="n"> attach-session </span><span class="nf">-t</span><span class="n"> </span><span class="o">$</span><span class="n">TMUX_CON_SESSION</span>
<span class="n">        else</span>
<span class="n">            tmux </span><span class="mi">-2</span><span class="n"> new-session </span><span class="nf">-s</span><span class="n"> </span><span class="o">$</span><span class="n">TMUX_CON_SESSION</span>
<span class="n">        fi</span>
<span class="n">    else</span>
<span class="n">        </span><span class="c1"># If inside tmux session then print MOTD</span>
<span class="n">        </span><span class="k">MOTD</span><span class="o">=</span><span class="n">/etc/motd.tcl</span>
<span class="n">        if [ </span><span class="nf">-f</span><span class="n"> </span><span class="o">$</span><span class="n">MOTD ]</span><span class="o">;</span><span class="n"> then</span>
<span class="n">            </span><span class="o">$</span><span class="n">MOTD</span>
<span class="n">        fi</span>
<span class="n">    fi</span>
<span class="o">}</span>
</pre></div>
</td></tr></table>
                            </div>
                            
                            <div class="overlay">&nbsp;</div>
                            
                            <div class="minimize">
                                <p>x</p>
