<span class="n">  </span><span class="c1"># { &quot;full_text&quot; : &quot;■ DISK: ${fs_used /}/${fs_size /} - [${fs_bar}]&quot; , &quot;color&quot; : &quot;\#00A9B5&quot;},</span>
<span class="n">  </span><span class="o">{</span><span class="n"> </span><span class="s">&quot;full_text&quot;</span><span class="n"> </span><span class="o">:</span><span class="n"> </span><span class="s">&quot;■ DISK: ${fs_used /} - [${fs_bar}]&quot;</span><span class="n"> , </span><span class="s">&quot;color&quot;</span><span class="n"> </span><span class="o">:</span><span class="n"> </span><span class="s">&quot;\#00A9B5&quot;</span><span class="o">}</span><span class="n">,</span>
