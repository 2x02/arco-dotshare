<span class="c1"># User ctrl-z like alt-tab</span>
<span class="k">fancy-ctrl-z</span><span class="n"> () </span><span class="o">{</span>
<span class="n">  if [[ </span><span class="o">$</span><span class="c1">#BUFFER -eq 0 ]]; then</span>
<span class="n">    </span><span class="k">BUFFER</span><span class="o">=</span><span class="s">&quot;fg&quot;</span>
<span class="n">    zle accept-line</span>
<span class="n">  else</span>
<span class="n">    zle push-input</span>
<span class="n">    zle clear-screen</span>
<span class="n">  fi</span>
<span class="o">}</span>
<span class="k">zle</span><span class="n"> </span><span class="nf">-N</span><span class="n"> fancy-ctrl-z</span>
<span class="k">bindkey</span><span class="n"> </span><span class="s">&#39;^Z&#39;</span><span class="n"> fancy-ctrl-z</span>

