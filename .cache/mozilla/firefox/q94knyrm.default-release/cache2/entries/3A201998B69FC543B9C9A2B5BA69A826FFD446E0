<span class="n">  </span><span class="o">{</span><span class="n"> </span><span class="s">&quot;full_text&quot;</span><span class="n"> </span><span class="o">:</span><span class="n"> </span><span class="s">&quot;≣ MEM: ${mem} &quot;</span><span class="n"> , </span><span class="s">&quot;color&quot;</span><span class="n"> </span><span class="o">:</span><span class="n"> </span><span class="s">&quot;\#C8C039&quot;</span><span class="n"> </span><span class="o">}</span><span class="n">,</span>
